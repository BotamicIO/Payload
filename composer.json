{
    "name": "faustbrian/payload",
    "description": "Serialisers/Deserialisers for several data-types",
    "keywords": [
        "payload",
        "Payload",
        "Brian Faust",
        "faustbrian"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6 || ^7.0",
        "league/csv": "^8.0",
        "netresearch/jsonmapper": "^1.00",
        "symfony/property-access": "^2.7|^3.0",
        "symfony/serializer": "^2.7|^3.0",
        "symfony/yaml": "^2.7|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Payload\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Payload\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
